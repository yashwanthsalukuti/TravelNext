<% layout("./layouts/boilerplate")%>
<div class="row">
  <br />
  <div class="col-6 offset-3">
    <br />
    <h2>Log In</h2>
    <br />
    <form method="post" action="/login" novalidate class="needs-validation">
      <div class="mb-3">
        <lable for="title" class="form-label"> username </lable>
        <input
          type="text"
          name="username"
          id="username"
          class="form-control"
          required
        />
      </div>
      <div class="mb-3">
        <lable for="title" class="form-label"> Password </lable>
        <input
          type="password"
          name="password"
          id="password"
          class="form-control"
          required
        />
      </div>
      <br />
      <button class="btn btn-add">Log In</button>
      <br /><br />
    </form>
  </div>
</div>
